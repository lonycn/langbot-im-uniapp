#app,
page,
view,
label {
  display: flex;
  flex-shrink: 0;
  flex-grow: 0;
  flex-basis: auto;
  align-items: stretch;
  align-content: flex-start;
}
button {
  margin: 0;
}
page,
view,
label,
swiper-item {
  flex-direction: column;
}
page,
view,
image,
input,
swiper,
scroll-view,
swiper-item,
text,
textarea,
video {
  box-sizing: border-box;
}
swiper-item {
  position: absolute;
}
.group-members-box {
  width: 750rpx;
  background-color: #f5f5f5;
}
.group-members-box .members-list-container {
  width: 750rpx;
  flex-direction: row;
  flex-wrap: wrap;
}
.group-members-box .members-list-container .item {
  position: relative;
  width: 150rpx;
  height: 140rpx;
  margin: 5px 0;
  align-items: center;
  justify-content: space-around;
}
.group-members-box .members-list-container .invite-icon {
  border: 1px dashed #ccc;
  border-radius: 10px;
  width: 90rpx;
  height: 90rpx;
  justify-content: center;
  cursor: pointer;
}
.group-members-box .item.focus {
  border: 1px dashed #ccc;
}
.group-members-box .group-admin {
  position: absolute;
  top: 5px;
  right: 5px;
  padding: 1px 3px;
  border-radius: 6px;
  background-color: #e64141;
  color: #fff;
  font-size: 12px;
}
.group-members-box .mute-type-1 {
  position: absolute;
  padding: 1px 10px;
  background-color: #0b60ff;
  color: #fff;
  font-size: 10px;
  bottom: 28px;
}
.group-members-box .avatar {
  width: 100rpx;
  height: 100rpx;
  border-radius: 10px;
  box-shadow: 0 0 1px #aaa;
}
.group-members-box .invite-text, .group-members-box .nickname {
  width: 140rpx;
  text-align: center;
  font-size: 12px;
  color: #666;
  padding: 0 16rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.group-members-box .tip {
  text-align: center;
  font-size: 14px;
  color: #666;
  height: 60px;
  line-height: 60px;
}
.group-members-box .set-index-box {
  flex-direction: row;
  justify-content: center;
}
.group-members-box .set-index-box .look-more-box {
  flex-direction: row;
  justify-content: center;
}
.group-members-box .set-index-box .item {
  margin: 0 10px;
  text-align: center;
  font-size: 14px;
  color: #666;
  height: 40px;
  line-height: 40px;
}
.group-members-box .invite-page {
  border-radius: 10px;
  background-color: #fff;
  overflow: hidden;
}
.group-members-box .invite-page .header-box {
  padding-top: 5px;
}