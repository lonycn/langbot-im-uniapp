.code-view-box {
  /*!
    Theme: GitHub Dark
    Description: Dark theme as seen on github.com
    Author: github.com
    Maintainer: @Hirse
    Updated: 2021-05-15

    Outdated base version: https://github.com/primer/github-syntax-dark
    Current colors taken from GitHub's CSS
  */
}
.code-view-box .hljs {
  display: block;
  overflow-x: auto;
  padding: 1em;
}
.code-view-box .hljs {
  padding: 3px 5px;
}
.code-view-box .hljs {
  color: #c9d1d9;
  background: #0d1117;
}
.code-view-box .hljs-doctag, .code-view-box .hljs-keyword, .code-view-box .hljs-meta .hljs-keyword, .code-view-box .hljs-template-tag, .code-view-box .hljs-template-variable, .code-view-box .hljs-type, .code-view-box .hljs-variable.language_ {
  color: #ff7b72;
}
.code-view-box .hljs-title, .code-view-box .hljs-title.class_, .code-view-box .hljs-title.class_.inherited__, .code-view-box .hljs-title.function_ {
  color: #d2a8ff;
}
.code-view-box .hljs-attr, .code-view-box .hljs-attribute, .code-view-box .hljs-literal, .code-view-box .hljs-meta, .code-view-box .hljs-number, .code-view-box .hljs-operator, .code-view-box .hljs-selector-attr, .code-view-box .hljs-selector-class, .code-view-box .hljs-selector-id, .code-view-box .hljs-variable {
  color: #79c0ff;
}
.code-view-box .hljs-meta .hljs-string, .code-view-box .hljs-regexp, .code-view-box .hljs-string {
  color: #a5d6ff;
}
.code-view-box .hljs-built_in, .code-view-box .hljs-symbol {
  color: #ffa657;
}
.code-view-box .hljs-code, .code-view-box .hljs-comment, .code-view-box .hljs-formula {
  color: #8b949e;
}
.code-view-box .hljs-name, .code-view-box .hljs-quote, .code-view-box .hljs-selector-pseudo, .code-view-box .hljs-selector-tag {
  color: #7ee787;
}
.code-view-box .hljs-subst {
  color: #c9d1d9;
}
.code-view-box .hljs-section {
  color: #1f6feb;
  font-weight: 700;
}
.code-view-box .hljs-bullet {
  color: #f2cc60;
}
.code-view-box .hljs-emphasis {
  color: #c9d1d9;
  font-style: italic;
}
.code-view-box .hljs-strong {
  color: #c9d1d9;
  font-weight: 700;
}
.code-view-box .hljs-addition {
  color: #aff5b4;
  background-color: #033a16;
}
.code-view-box .hljs-deletion {
  color: #ffdcd7;
  background-color: #67060c;
}
.code-view-root {
  flex: 1;
  border-radius: 3px;
  background-color: #0d1117;
  width: 100%;
}
.copy-btn-box {
  font-size: 12px;
  top: 0px;
  flex-direction: row;
  justify-content: space-between;
  align-items: flex-end;
  padding: 5px;
}
.language {
  color: #888;
}
.copy-btn {
  font-size: 12px;
  padding: 0 5px;
  color: #888;
}
.code-view-box {
  position: relative;
  padding: 0;
  margin-bottom: 10px;
  background-color: #0d1117;
  padding: 0 5px;
  overflow: auto;
  width: 100%;
  box-sizing: border-box;
}
.code-view-rich-text {
  font-size: 14px;
  border-radius: 10px;
}
.msg-code {
  width: 600rpx;
}
.show-full-btn {
  justify-content: center;
  align-items: center;
  height: 30px;
  border-top: 1px solid #30363d;
}
.show-full-text {
  margin: 0 auto;
  font-size: 12px;
  color: #888;
}