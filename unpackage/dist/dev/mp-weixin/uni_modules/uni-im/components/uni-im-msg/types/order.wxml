<view class="msg-order"><text class="title">订单信息</text><view class="item"><text class="title">订单编号：</text><text selectable="{{true}}" class="value">{{a}}</text></view><view class="item"><text class="title">产品/服务名称：</text><text class="value">{{b}}</text></view><view class="item"><text class="title">订单价格：</text><text class="value">{{c}}元</text></view><view class="item"><text class="title">所属用户：</text><text class="value">{{d}}</text></view><view class="item"><text class="title">订单状态：</text><text class="value">{{e}}</text></view><block wx:if="{{f}}"><view class="item"><text class="title">支付时间：</text><text class="value">{{g}}</text></view><view class="item"><text class="title">支付渠道：</text><text class="value">{{h}}</text></view></block><view wx:elif="{{i}}" class="btns"><button class="btn" type="primary" bindtap="{{j}}" disabled="{{k}}">在线支付</button><text class="tip">在线支付，需在 {{l}} 前完成</text><button class="btn" type="primary" bindtap="{{m}}" disabled="{{n}}">对公打款</button><text class="tip">对公打款，需在 {{o}} 前完成</text></view></view>